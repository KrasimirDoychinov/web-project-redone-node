<header>
	<a id="header-title" href="/"
		><img
			src="https://res.cloudinary.com/detha4545/image/upload/v1665658776/swtor/logo_hrsqx4.png"
			alt=""
	/></a>
	<ul>
		<% if (!isLoggedIn) { %>
		<li><a href="/auth/register">Register</a></li>
		<li><a href="/auth/login">Login</a></li>
		<% } else { %>
		<a href="/user/profile"><img src="<%= user.imageUrl %>" alt="" /></a>
		<li><a href="/auth/logout">Logout</a></li>
		<% } %>
	</ul>
</header>
